{
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "next": "^15.5.6",
    "nodemon": "^3.1.10",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@agoralabs-sh/avm-web-provider": "^1.7.0",
    "@blockshake/defly-connect": "^1.2.1",
    "@perawallet/connect": "^1.4.2",
    "@prisma/client": "^6.17.1",
    "@randlabs/myalgo-connect": "^1.4.2",
    "@tailwindcss/postcss": "^4.1.16",
    "@txnlab/use-wallet": "^4.3.1",
    "@walletconnect/modal": "^2.7.0",
    "@walletconnect/sign-client": "^2.22.4",
    "@walletconnect/types": "^2.22.4",
    "algosdk": "^3.5.2",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "colyseus": "^0.16.5",
    "express": "^5.1.0",
    "framer-motion": "^11.11.10",
    "jsonwebtoken": "^9.0.2",
    "lute-connect": "^1.6.3",
    "mongodb": "^6.20.0",
    "nanoid": "^5.1.6",
    "phaser": "^3.90.0",
    "prisma": "^6.17.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwindcss": "^4.1.16"
  },
  "overrides": {
    "@colyseus/core": {
      "nanoid": "^3.3.8"
    }
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npx next dev",
    "dev:backend": "cd backend && npx nodemon --watch routes --watch server.js --ext js,json server.js",
    "build": "cd frontend && npx next build",
    "start": "npm run start:frontend",
    "start:frontend": "cd frontend && npx next start",
    "start:backend": "cd backend && node server.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:deploy": "npx prisma migrate deploy"
  }
}
