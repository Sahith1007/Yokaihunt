{
  "devDependencies": {
    "concurrently": "^9.2.1",
    "next": "^15.5.6",
    "nodemon": "^3.1.10",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "colyseus": "^0.14.17",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "phaser": "^3.90.0",
    "prisma": "^6.17.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "framer-motion": "^11.11.10"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:server\"",
    "dev:frontend": "cd frontend && npx next dev",
    "dev:server": "cd server && npx nodemon index.js",
    "build": "cd frontend && npx next build",
    "start": "cd frontend && npx next start",
    "server": "node server/index.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:deploy": "npx prisma migrate deploy"
  }
}
